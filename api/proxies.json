{
    "$schema": "http://json.schemastore.org/proxies",
    "proxies": {
        "buildproxy": {
            "matchCondition": {
                "route": "{*path}",
                "methods": [
                    "GET"
                ]
            },
            "backendUri": "https://%WEBSITE_HOSTNAME%/api/build/{path}"
        }
    }
}